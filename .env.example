# Dream Journey Frontend - .env.example
# Copy this file to `.env` and fill in the real values.

# Frontend Supabase Configuration
VITE_SUPABASE_PROJECT_ID="YOUR_PROJECT_ID"
VITE_SUPABASE_URL="https://YOUR_PROJECT_ID.supabase.co"
VITE_SUPABASE_PUBLISHABLE_KEY="your_anon_key"
VITE_SUPABASE_ANON_KEY="your_anon_key"

# Backend URLs
VITE_BACKEND_URL="http://localhost:3001/api"
VITE_FRONTEND_URL="http://localhost:5173"

# Optional providers / APIs
VITE_RUNWAY_API_KEY=

# Admin (frontend may reference for UI hints; authorization enforced on backend)
ADMIN_EMAILS="admin1@example.com,admin2@example.com"

# Email (frontend does not need credentials; shown here for clarity only)
EMAIL_USER=
EMAIL_PASSWORD=

# NLP (optional)
NLP_URL=http://localhost:5000

# Video (optional)
RUNWAY_API_KEY=
PIKA_API_KEY=
KAIBER_API_KEY=

# SMS (optional for future)
TWILIO_ACCOUNT_SID=
TWILIO_AUTH_TOKEN=
TWILIO_FROM=
